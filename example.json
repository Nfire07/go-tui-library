{
  "layout": "flex",
  "flexDirection": "column",
  "align": "center",
  "justify": "center",
  "elements": [
    {
      "type": "div",
      "id": "content",
      "layout": "flex",
      "flexDirection": "column",
      "align": "center",
      "justify": "center",
      "width": 178,
      "height": 44,
      "style": {
        "border": "rounded"
      },
      "children": [
        {
          "type": "text",
          "id": "form-title",
          "value": "Login",
          "style": {
            "color": "#fefefe"
          }
        },
        {
          "type": "input",
          "id": "username-input",
          "label": "Username",
          "value": "",
          "width": 40,
          "inputType": "text",
          "style": {
            "border": "rounded",
            "color": "#c2c2c2",
            "background": "#121212"
          }
        },
        {
          "type": "input",
          "id": "password-input",
          "label": "Password",
          "value": "",
          "width": 40,
          "inputType": "password",
          "style": {
            "border": "rounded",
            "color": "#c2c2c2",
            "background": "#121212"
          }
        },
        {
          "type": "input",
          "id": "cvd-input",
          "label": "cvd",
          "value": "pls",
          "width": 20,
          "inputType": "number",
          "style": {
            "border": "rounded",
            "color": "#c2c2c2"
          }
        },
        {
          "type": "checkbox",
          "id": "agree-checkbox",
          "label": "I agree to terms",
          "checked": false,
          "style": {
            "color": "#fefefe"
          }
        },
        {
          "type": "button",
          "id": "submit-btn",
          "label": "Submit",
          "width": 50,
          "style": {
            "border": "rounded",
            "color": "#fefefe"
          }
        }
      ]
    }
  ]
}